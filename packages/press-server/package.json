{
  "name": "@collected/press-server",
  "version": "0.5.0",
  "description": "Render and serve Markdown directly from a GitHub repo",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/module.js",
      "import": "./dist/module.js",
      "require": "./dist/main.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "npm run build",
    "build": "parcel build",
    "test": "jest && tsc --noEmit && parcel build",
    "dev": "wrangler dev tests/worker.ts"
  },
  "author": "Patrick Smith",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "date-fns": "^2.30.0",
    "itsybitsy": "^0.1.1",
    "markdown-it": "^13.0.1",
    "markdown-it-front-matter": "^0.2.3",
    "markdown-it-highlightjs": "^3.6.0",
    "markdown-it-task-lists": "^2.1.1",
    "mrmime": "^1.0.1",
    "vhtml": "^2.2.0",
    "yaml": "^2.3.2",
    "yieldparser": "^0.3.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230914.0",
    "@parcel/packager-ts": "^2.9.3",
    "@parcel/transformer-typescript-types": "^2.9.3",
    "@swc/core": "^1.3.86",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.5",
    "chai": "^4.3.8",
    "html-rewriter-wasm": "^0.4.1",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "parcel": "^2.9.3",
    "typescript": "^4.9.5",
    "wrangler": "^3.18.0"
  }
}
