{
  "name": "@collected/press-server",
  "version": "0.2.2",
  "description": "Render and serve Markdown directly from a GitHub repo",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/module.js",
      "import": "./dist/module.js",
      "require": "./dist/main.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "npm run build",
    "build": "parcel build",
    "test": "jest && tsc --noEmit && parcel build",
    "dev": "wrangler dev tests/worker.ts"
  },
  "author": "Patrick Smith",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "date-fns": "^2.29.3",
    "itsybitsy": "^0.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-front-matter": "^0.2.3",
    "markdown-it-highlightjs": "^3.5.0",
    "markdown-it-task-lists": "^2.1.1",
    "mrmime": "^1.0.0",
    "vhtml": "^2.2.0",
    "yaml": "^2.1.3",
    "yieldparser": "^0.3.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@parcel/packager-ts": "^2.8.0",
    "@parcel/transformer-typescript-types": "^2.8.0",
    "@swc/core": "^1.3.19",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^29.2.3",
    "chai": "^4.3.7",
    "html-rewriter-wasm": "^0.4.1",
    "jest": "^29.3.1",
    "mocha": "^10.1.0",
    "parcel": "^2.8.0",
    "typescript": "^4.9.3",
    "wrangler": "^2.4.4"
  }
}
